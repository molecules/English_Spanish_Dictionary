head -n1 dictionary.tsv > sound_dict.tsv
tail -n +2 dictionary.tsv | sort >> sound_dict.tsv 
head -n1 dictionary.tsv > sound_spelling_dict.tsv
tail -n +2 dictionary.tsv | sort -k2 >> sound_spelling_dict.tsv 
head -n1 dictionary.tsv > word_spelling_dict.tsv
tail -n +2 dictionary.tsv | sort -k3 >> word_spelling_dict.tsv 
head -n1 dictionary.tsv > pronunciation_dict.tsv
tail -n +2 dictionary.tsv | sort -k4 >> pronunciation_dict.tsv

cp sound_dict.tsv dictionary.tsv
